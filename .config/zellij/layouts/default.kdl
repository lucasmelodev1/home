layout {
  cwd "~"
  tab name="editor" focus=true cwd="~/projects"
  tab name="sh" split_direction="vertical" cwd="~/projects"
  tab name="notes" split_direction="vertical" cwd="~/notes"
  tab name="sh-2" split_direction="vertical"
  tab name="sh-3" split_direction="vertical"
  tab name="docker-cont" split_direction="vertical"
  default_tab_template {
    children
    pane size=1 borderless=true {
        plugin location="zellij:compact-bar"
    }
  }
}
